{
  "manifest_version": 3,
  "name": "OmniX",
  "version": "1.0.0",
  "action": {},
  "content_scripts": [
    {
      "js": ["src/content/index.tsx"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "permissions": ["tabs", "activeTab", "search", "bookmarks", "browsingData"]
}
